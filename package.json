{
  "name": "fluent-ui3",
  "version": "0.0.1",
  "type": "module",
  "description": "a fluentui library base on vue3 + TS",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -C playground dev",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fluent-ui3/components": "workspace:*",
    "@fluent-ui3/hooks": "workspace:*",
    "@fluent-ui3/theme-chalk": "workspace:*",
    "@fluent-ui3/utils": "workspace:*",
    "@vueuse/core": "^9.11.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@types/node": "^18.11.18",
    "@vue/runtime-core": "^3.2.45",
    "@vue/shared": "^3.2.45",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "lint-staged": "^13.1.0",
    "rimraf": "^4.1.1",
    "sass": "^1.57.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "unplugin-vue-define-options": "^1.1.5",
    "vite": "^4.0.4",
    "vitest": "^0.27.3",
    "vue": "^3.2.45"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  }
